{
  "name": "setup-umka-action",
  "version": "1.1.3",
  "description": "This GitHub action installs Umka",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-umka-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint:md": "markdownlint --config .markdownlint.yml '**/*.md'",
    "lint:es": "eslint --ext ts src",
    "lint": "yarn run lint:md && yarn run lint:es",
    "postinstall": "yarn husky install",
    "security:auth": "snyk auth",
    "security:test": "snyk test",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "programming-language",
    "scripting-language",
    "umka"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-umka-action#readme",
  "dependencies": {
    "@actions/core": "1.8.2",
    "@actions/tool-cache": "2.0.1",
    "semver": "7.3.7",
    "winston": "3.7.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/glob": "7.2.0",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.34",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "5.24.0",
    "@typescript-eslint/parser": "5.24.0",
    "@vercel/ncc": "0.33.4",
    "chai": "4.3.6",
    "eslint": "8.15.0",
    "eslint-config-google": "0.14.0",
    "git-branch-is": "4.0.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-circus": "28.1.0",
    "markdownlint-cli": "0.31.1",
    "mocha-param": "2.0.1",
    "snyk": "1.931.0",
    "ts-jest": "28.0.2",
    "typescript": "4.6.4"
  },
  "snyk": true
}
